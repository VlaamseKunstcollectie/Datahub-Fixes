            )

        end

        if all_match(t.textTypeID, '128')

            lido_basevalue(
                descriptiveMetadata.eventWrap.eventSet.$last.event.eventMaterialsTech.$last.displayMaterialsTech,
                t.textEntry,
                -lang: fr
            )

        end

    end

